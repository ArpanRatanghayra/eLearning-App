<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.marmag">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".QuizActivity"></activity>
        <activity android:name=".SideNavActivity" />
        <activity android:name=".ReferralActivity" />
        <activity android:name=".WalletActivity" />
        <activity android:name=".PaymentActivity" />
        <activity android:name=".EmailActivity" />
        <activity android:name=".SlotActivity" />
        <activity android:name=".SelectSlotActivity" />
        <activity android:name=".SelectCourseActivity" />
        <activity android:name=".CourseActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".ClassroomActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>